<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>        
        .eastPanel {
            background-color: grey;
        }
        .westPanel {
            background-color: grey;
        }
        .northPanel {
            background-color: grey;
        }
        .southPanel {
            background-color: grey;
        }
        .centerPanel {
            background-color: grey;
        }       
    </ui:style>
     <g:DockLayoutPanel unit='EM'>
        <g:north size='3.0'>
         <g:FlowPanel styleName="{style.northPanel}">
         	<g:Button ui:field="adminAccount" width="150px">Administrere Konto</g:Button>
         	<g:Button ui:field="emailAdmin" width="150px">Email Administrator</g:Button>
         	<g:Button ui:field="buttonLogout" width="150px">Logg Ut</g:Button>
         </g:FlowPanel>
       </g:north>
       <g:west size='12.1'>
         <g:FlowPanel styleName="{style.westPanel}">
         	<g:TextBox text="Navn" ui:field="name" width="140px"/>
         	<g:TextBox text="Påvirkningsgrad" ui:field="impactFactor" width="140px"/>
         	<g:TextBox text="Effekt" ui:field="effect" width="140px"/>
         	<g:TextBox text="Volt" ui:field="volt" width="140px"/>
         	<g:TextBox text="Lengdegrader" ui:field="longtitude" width="140px"/>
         	<g:TextBox text="Breddegrader" ui:field="latitude" width="140px"/>
         	<g:TextBox text="Forbrukermønster" ui:field="usagePattern" width="140px"/>
         	<g:Button width="150px" ui:field="back">Tilbake</g:Button>
         	<g:Button width="150px" ui:field="addObject">Legg Til Objektet</g:Button>
         </g:FlowPanel>
       </g:west>
       <g:center>
         <g:FlowPanel styleName="{style.centerPanel}">
         	<g:Button ui:field="saveObject" width="140px">Lagre Objekt</g:Button>
         </g:FlowPanel>
       </g:center>
       <g:east size='11.4'>
         <g:FlowPanel styleName="{style.eastPanel}">
         </g:FlowPanel>
       </g:east>     
        <g:south size="5">
         <g:FlowPanel styleName="{style.southPanel}">
         </g:FlowPanel>
        </g:south>       
     </g:DockLayoutPanel>
</ui:UiBinder> 